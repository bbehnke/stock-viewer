{
  "name": "stock-viewer",
  "version": "1.0.0",
  "author": "Bradley Behnke",
  "license": "ISC",
  "description": "Simple standalone stock-viewer app.",
  "repository": {
    "type": "git",
    "url": "https://github.com/bbehnke/stock-viewer.git"
  },
  "main": "src/server/index.js",
  "scripts": {
    "test": "jest",
    "start-client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "start-server": "nodemon src/server/index.js",
    "start": "concurrently \"npm run start-server\" \"npm run start-client\""
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./setupTests.js",
      "./node_modules/regenerator-runtime/runtime"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "lowdb": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^5.0.0",
    "recharts": "^1.5.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-saga": "^1.0.0",
    "reselect": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
